local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Slap Battles | Troll Script | Whale Scripts", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "When The Saints Go Marching",
	Content = "Coded By @WhaleRoblox On YT",
	Image = "rbxassetid://4483345998",
	Time = 5
})

OrionLib:MakeNotification({
	Name = "Fun Fact!",
	Content = "i wont bitch around when you upload my scripts",
	Image = "rbxassetid://4483345998",
	Time = 8
})

----- friwontitsafunfact

local FSTab = Window:MakeTab({
	Name = "Free Stuff",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FSTab:AddLabel("if your a showcaser and have no slaps use this ig")
FSTab:AddLabel("if your ALSO a showcaser check out the settings tab")

FSTab:AddButton({
	Name = "Rhythm Explosion",
	Callback = function()
      		game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
  	end    
})

FSTab:AddButton({
	Name = "Null's Ability",
	Callback = function()
      		game:GetService("ReplicatedStorage").NullAbility:FireServer()
  	end    
})

FSTab:AddButton({
	Name = "Here Comes The Piano [Blasphemy]",
	Callback = function()
      		game:GetService("ReplicatedStorage").Piano:FireServer()
  	end    
})

FSTab:AddLabel("Custom Abilitys For Free Lol")

FSTab:AddButton({
	Name = "Rocket-Ship",
	Callback = function()
      		game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
              wait(0.01)
         loadstring(game:HttpGet('https://raw.githubusercontent.com/LoserGrape/flinger-roblox/main/chatgpt%20fart'))()
  	end    
})

FSTab:AddButton({
	Name = "Death Wish",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/LoserGrape/flinger-roblox/main/chatgpt%20fart'))()
              wait(0.01)
         game:GetService("Players").LocalPlayer.Reset:FireServer()
  	end    
})

FSTab:AddButton({
	Name = "Active Bomb Threat",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game.ReplicatedStorage.Assets.Thor.Animation, game.Players.LocalPlayer.Character.Humanoid):Play()
              wait(2.1)
            game:GetService("ReplicatedStorage").RetroAbility:FireServer("Bomb")
         wait(0.03)
     game:GetService("Players").LocalPlayer.Reset:FireServer()
  	end    
})

local CombatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

CombatTab:AddToggle({
	Name = "Hitbox",
	Default = false,
	Callback = function(Value)
_G.HitboxPlayer = Value
while _G.HitboxPlayer do
for i,v in pairs(game.Players:GetChildren()) do
                    if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                        v.Character.HumanoidRootPart.Size = Vector3.new(_G.ReachHitbox,_G.ReachHitbox,_G.ReachHitbox)
                        v.Character.HumanoidRootPart.Transparency = 0.75
                    end
                end
task.wait()
end
